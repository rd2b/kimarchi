;
; BIND data file for local loopback interface
;
$TTL    604800
@       IN      SOA     acgform.local. dns01.acgform.local. (
                              1         ; Serial
                         604800         ; Refresh
                          86400         ; Retry
                        2419200         ; Expire
                         604800 )       ; Negative Cache TTL
;
@       IN      NS      dns01.acgform.local.
@       IN      NS      dns02.acgform.local.
@       IN      A       10.0.0.4
ns      IN      A       10.0.0.4
dns01   IN      A       10.0.0.4
dns02   IN      A       10.0.0.4

cf01	IN      A       10.0.0.2
perso01	IN      A       10.0.0.3
drop01	IN      A       10.0.0.5
dev01	IN      A       10.0.0.6
torrent01	IN      A       10.0.0.7
log01	IN      A       10.0.0.8


