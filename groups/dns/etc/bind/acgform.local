;
; BIND data file for local loopback interface
;
$TTL    604800
@       IN      SOA     acgform.local. dns01.acgform.local. (
                              1         ; Serial
                         604800         ; Refresh
                          86400         ; Retry
                        2419200         ; Expire
                         604800 )       ; Negative Cache TTL
;
@       IN      NS      dns01.acgform.local.
@       IN      A       192.168.122.10
ns      IN      A       192.168.122.10
dns01   IN      A       192.168.122.10
bast01	IN      A       192.168.122.11
cft01	IN      A       192.168.122.12


dep01	IN	A	192.168.122.60
drop01	IN	A	192.168.122.61
log01	IN	A	192.168.122.62


vm01	IN	A	192.168.122.101
vm02	IN	A	192.168.122.102

db01    IN      A       192.168.122.111
db02    IN      A       192.168.122.112

front01	IN	A	192.168.122.121
front02	IN	A	192.168.122.122

vip-db  IN      CNAME   db01.acgform.local.





